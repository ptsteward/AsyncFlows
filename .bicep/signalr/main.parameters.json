                {
                    "region": "westus2",
                    "tier": "Standard",
                    "upstreamUrlTemplate": "https://api.concurrentflows.com/api/{hub}/{category}/{event}",
                    "capacity": 1
                },
                {
                    "region": "centralus",
                    "tier": "Standard",
                    "upstreamUrlTemplate": "https://api.concurrentflows.com/api/{hub}/{category}/{event}",
                    "capacity": 1
                }
            ]
        }
    }
